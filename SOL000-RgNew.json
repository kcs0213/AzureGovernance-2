{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "ResourceGroupNameIndividual": {
      "type": "string"
    },
    "SubscriptionName": {
      "type": "string"
    },
    "Region": {
      "type": "string",
      "allowedValues": [
        "westeurope",
        "northeurope"
      ]
    },
    "ApplicationId": {
      "type": "string"
    },
    "CostCenter": {
      "type": "string"
    },
    "Budget": {
      "type": "string"
    },
    "Contact": {
      "type": "string"
    }
  },

  "variables": {

  },

  "resources": [
    {
      "name": "linkedTemplate",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2018-05-01",
      "location": "westeurope",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "https://raw.githubusercontent.com/fbodmer/AzureGovernance/master/PAT0000-ResourceGroupNew.json"
        },
        "parameters": {
          "ResourceGroupNameIndividual": "[parameters('ResourceGroupNameIndividual')]",
          "SubscriptionName": "[parameters('SubscriptionName')]",
          "Region": "[parameters('Region')]",
          "ApplicationId": "[parameters('ApplicationId')]",
          "CostCenter": "[parameters('CostCenter')]",
          "Budget": "[parameters('Budget')]",
          "Contact": "[parameters('Contact')]",
          "Automation": "1.0"
        }
      }
    }
  ]
}